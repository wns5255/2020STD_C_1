#include <iostream>
using namespace std;


int main(void) {
	int a, b, c, d;
	cin >> a >> b >> c >> d;
	int s = a + b + c + d;

	if (s < 60 || s > (59 * 60 + 59))
		return 0;

	cout << s / 60 << endl;
	cout << s % 60 << endl;
	return 0;
	
}


/*
초를 받아서 분으로 받아주기

input : a = 집-학교, b = 학교-PC방, c = PC방-학원, d = 학원-집

s=a+b+c+d

output : s/60
	s%60
*/
